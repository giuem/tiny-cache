{
  "name": "@giuem/tiny-cache",
  "version": "0.0.0",
  "description": "A tiny solution to cache your javascript files",
  "scripts": {
    "type-check": "tsc --noEmit",
    "build:types": "tsc --emitDeclarationOnly",
    "build:lib": "rollup -c",
    "build": "npm run build:types && npm run build:lib",
    "clean": "rimraf dist",
    "test": "jest"
  },
  "main": "dist/tiny-cache.cjs.js",
  "module": "dist/tiny-cache.esm.js",
  "browser": "dist/tiny-cache.iife.js",
  "types": "dist/types",
  "repository": "https://github.com/giuem/tiny-cache",
  "author": {
    "name": "GIUEM",
    "email": "giuem.com+npm@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-typescript": "^7.1.0",
    "@types/jest": "^23.3.13",
    "jest": "^24.0.0",
    "prettier": "^1.16.1",
    "rimraf": "^2.6.3",
    "rollup": "^1.1.0",
    "rollup-plugin-babel": "^4.3.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-terser": "^4.0.2",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.2.4"
  }
}
